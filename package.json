{
  "name": "@bednarik_radek/mockserver-cli",
  "version": "0.1.2",
  "description": "CLI utility for Mock-server client. Allows for easy creation/deletion of expectations on the Mock-server instance.",
  "main": "index.js",
  "type": "module",
  "bin": {
    "expectations": "dist/bin/expectations.js"
  },
  "keywords": [
    "mocks",
    "mockserver",
    "expectations",
    "cli",
    "mockserver-client",
    "mocks",
    "mocking",
    "development",
    "testing"
  ],
  "author": "Radek Bednarik<bednarik.radek@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/radekBednarik/mockserver-cli.git"
  },
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/radekBednarik/mockserver-cli/blob/main/README.md",
  "scripts": {
    "dev": "mockserver -logLevel DEBUG -serverPort 5999",
    "lint": "tsc --noEmit --pretty",
    "link": "cd ./dist && npm link && chmod +x ./bin/expectations.js && cd ..",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist && tsc && npm run link",
    "prepare": "husky install"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.3",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --cache"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2
  },
  "dependencies": {
    "commander": "^11.1.0",
    "mockserver-client": "^5.15.0",
    "p-queue": "^8.0.0",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3"
  }
}
